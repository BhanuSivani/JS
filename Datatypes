        /*let a = 1;
        console.log("hi + a+a")
        document.write('hi+a+a',"<br>")
        document.write(`hi+ ${a+a}`)
        console.log(`hi +${a+a}`)*/

        a= "sai";
        b= "kumar";
        age = 30;
        document.write(`I am+ ${a+" "+b},My age is ${age} years old`)
        console.log(`I am+ ${a+" "+b},My age is ${age} years old`)
    </script>
</body>
